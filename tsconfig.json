{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6",
    "types": [
      "node",
      "@types/webappsec-credential-management"
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "app/*"
      ]
    }
  }
}